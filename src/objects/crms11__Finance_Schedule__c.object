        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF( LEN ( 
             TEXT( 
                     YEAR( crms11__Date__c ) 
                 ) 
             ) == 4, 
    RIGHT( 
        TEXT( 
                YEAR( crms11__Date__c ) 
            ), 2) ,
    TEXT( YEAR( crms11__Date__c ) )
    )

+ &apos;/&apos; + 

    IF( LEN ( 
            TEXT( 
                    MONTH( crms11__Date__c ) 
                ) 
            ) == 1, 
    &apos;0&apos; + TEXT( MONTH( crms11__Date__c ) ),
    TEXT( MONTH( crms11__Date__c ) )
    )</formula>
