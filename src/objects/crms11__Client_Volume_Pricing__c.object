    <validationRules>
        <fullName>crms11__Apply_one_Pricing_at_a_time</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISBLANK(crms11__Fixed_Volume_Rate__c)), NOT(ISBLANK(crms11__Discount_Percentage__c)))</errorConditionFormula>
        <errorMessage>You may only enter one of Fixed Volume Price or Discount Percentage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>crms11__At_least_one_Pricing_should_be_enable</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISBLANK( crms11__Fixed_Volume_Rate__c ), ISBLANK( crms11__Discount_Percentage__c ))</errorConditionFormula>
        <errorMessage>You must enter either a Fixed Volume Price or a Discount Percentage.</errorMessage>
    </validationRules>
