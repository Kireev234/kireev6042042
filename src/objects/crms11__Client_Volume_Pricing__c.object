    <fields>
        <fullName>crms11__Client_Volume_Price__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF( (crms11__Fixed_Volume_Rate__c &lt;= 0 ), (crms11__Client_Price__r.crms11__Client_Price__c  *  crms11__Minimum_Quantity__c  *  crms11__Discount_Percentage__c), crms11__Fixed_Volume_Rate__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Client Volume Price</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
